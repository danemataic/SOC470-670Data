vote <- read_csv("https://raw.github.com/danemataic/SOC470-670Data/master/american_voter_survey.csv")
vote <- vote %>%
mutate(live.alone = as.factor(case_when(
V161109 == 0 ~ "alone",
V161109 > 0 ~ "others")))
vote <- vote %>%
mutate(political.ideology = as.factor(case_when(
V161126 <= 4 ~ "Liberal or Moderate",
V161126 > 4 ~ "Conservative")))
ggplot(vote, aes(x=political.ideology))+
geom_bar()
ggplot(vote, aes(x=live.alone))+
geom_bar()
t.test(V162100 ~ live.alone, data=vote)
vote <- vote %>%
mutate(live.threegroup = as.factor(case_when(
V161109 == 0 ~ "alone",
